<% tracks.forEach(function(track) { %>
  <div class="card" style="margin-top: 5px;">
    <h5 class="card-header">
      <%= track.platform %>
    </h5>
    <div class="card-body">
      <h5 class="card-title"><%= track.label %></h5>
      <p class="card-text">Target Price: <%= track.targetPrice %> <br> Original Price: <%= track.originalPrice %> </p>
      <a href="<%= track.url %>" target="_blank" class="btn btn-primary">Go</a>
      <a href="#" onclick="(() => { navigator.clipboard.writeText('<%= track.url %>');})()" class="btn btn-primary">Copy</a>
    </div>
  </div>
<% }) %>
